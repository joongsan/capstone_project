            <p>
                
                <div>
                  
                  <h2 style="margin:20px; float:left;">Write Your Story</h2>
                  <div id="bb-social-media-widget" style="line-height:12px; float:right">
                    <div>
                        <!-- facebook share current page -->
                        <div class="fb-share-button" data-layout="button" data-size="small" data-mobile-iframe="true"><a class="fb-xfbml-parse-ignore" target="_blank" href="https://www.facebook.com/sharer/sharer.php?u&amp;src=sdkpreparse">Share</a></div>
                        
                        <!-- twitter share current page -->
                        <a href="https://twitter.com/share" class="twitter-share-button" data-hashtags="Expedia"data-count="vertical">Tweet</a>
                    
                        <!-- google plus share current page -->
                        <div class="g-plus" data-action="share" data-annotation="none"></div>
                    </div>
                    &emsp;&emsp;&emsp;&emsp;
                    <a class="btn btn-default" href="/users/sign_out" role="button" style="float:right;">Sign Out</a>
                </div>
                </div>
                    <form style="margin-top:10px; margin-right:20px; margin-left:20px;" action="/home/write" method="POST" enctype="multipart/form-data">
                      <div class="form-group" >
                          <input type="text" name="title" class="form-control" id="exampleInputEmail1" placeholder="Title">
                          
                          <textarea style="margin-top:5px;" class="form-control" rows="3" id="content" name="content" placeholder="Content"></textarea>
                          <input style="display:inline-block; font-size:14px; margin-top:5px;" type="file" name="pic" accept="image/*">
                          <button style="display:inline-block; margin-top:5px; margin-right:0px; float:right;" type="submit" class="btn btn-default">Submit</button>
                      </div>
                    </form> <hr>
                    
                    
                      <% @posts.each do |p| %>
                      <% next if p.user.email == "wdntks123@naver.com" %>
                          <div class="well">
                              <h2><%= p.title %>
                              <small style="float:right;"> <%= p.user.email %></small></h2>
                              <hr>
                              <p> <%= p.content %> </br></br></p>
                              <% unless p.image_url.nil? %>
                              <img id= "myImg<%= p.id %>" src= "<%= p.image_url %>" class="img-responsive" height="200px" width="200px"></br>
                              <div id="myModal" class="modal">
                                  <span class="close">×</span>
                                  <img class="modal-content" id="img01">
                                  <div id="caption"></div>
                              </div>
                              <style>
                                #myImg<%= p.id %> {
                                    border-radius: 5px;
                                    cursor: pointer;
                                    transition: 0.3s;
                                    }
                                
                                #myImg<%= p.id %>:hover {opacity: 0.7;}
                              </style>
                              <script>
                                // Get the modal
                                var modal = document.getElementById('myModal');
                                
                                // Get the image and insert it inside the modal - use its "alt" text as a caption
                                var img = document.getElementById('myImg<%= p.id %>');
                                var modalImg = document.getElementById("img01");
                                var captionText = document.getElementById("caption");
                                img.onclick = function(){
                                    modal.style.display = "block";
                                    modalImg.src = this.src;
                                    captionText.innerHTML = this.alt;
                                }
                                
                                // Get the <span> element that closes the modal
                                var span = document.getElementsByClassName("close")[0];
                                
                                // When the user clicks on <span> (x), close the modal
                                span.onclick = function() {
                                    modal.style.display = "none";
                                }
                              </script>
                              <%end%>
                              <p style="float:right;"><%= p.created_at %> </p><hr>
                              <div id="reply_<%=p.id%>"></br>
                                <% p.comments.each do |c| %>
                                    <p ><small style="float:right;"><%= c.created_at %> </small>ㄴ&emsp;<%= c.content %></p></br>
                                <% end %>
                              </div>
                              
                              
                              <textarea class="form-control" rows="2" id="content_<%= p.id %>" name="comment_content" placeholder="Comment"></textarea>
                              
                              <button value="<%= p.id %>" type="submit" class="btn btn-default reply_submit">Comment</button>
                              
                          </div>
                      <% end %>
                    <script>
                    $(function(){
                      //reply_submit이 클릭되면
                      $(".reply_submit").click(function(){
                        
                        post_id = this.value;
                        post_content = $("#content_"+post_id).val();
                        
                        $.ajax({
                          method: "POST",
                          url: "/home/comment_write",
                          data: { comment_content: post_content, id_comment: post_id},
                          success: function( msg ) {
                            $("#reply_"+post_id).append("<p>ㄴ" + post_content + "<p>" );
                            $("#content_"+post_id).val('');
                          },
                          error: function( msg ) {
                            alert( "리플이 작성되지 않았습니다");
                          }
                        })
                      });
                    });
                    </script>
                </div>
